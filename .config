server {
    listen 8080;
    server_name webserv.com;

    root ./www;
    index index.html;

    error_page 404 /404.html;

    location / {
        try_files $uri $uri/ =404;
    }

    location /private/ {
        root ./www/secret;
        index secret.html;
    }

    location /cgi-bin/hello.py {
        cgi_pass /opt/homebrew/bin/python3 cgi-bin/hello.py;
    }

    location /upload_here {
        allow_methods POST;
        upload_path ./www/uploads;
        client_max_body_size 10M;
    }

    location /folder.html {
        root ./www;
        index folder.html;
    }
}
